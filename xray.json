{
  "log": {
    "loglevel": "debug",
    "access": "/dev/stdout",
    "error": "/dev/stdout"
  },
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": 3128,
      "protocol": "http",
      "settings": {
        "auth": "password",
        "accounts": [
          { "user": "myuser", "pass": "MyStrongPass123" }
        ]
      },
      "tag": "proxy-in",
      "sniffing": { "enabled": false },
      "outboundTag": "direct"
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "direct",
      "settings": { "domainStrategy": "UseIPv4" }
    }
  ]
}
