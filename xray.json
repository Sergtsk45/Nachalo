{
  "log": {
    "loglevel": "debug",
    "access": "/dev/stdout",
    "error": "/dev/stdout"
  },
  "inbounds": [
    {
      "port": 3128,
      "listen": "0.0.0.0",
      "protocol": "http",
      "settings": {
        "auth": "noauth"
      },
      "tag": "proxy-in",
      "sniffing": {
        "enabled": false
      },
      "allocate": {
        "strategy": "always"
      },
      "outboundTag": "direct"
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "direct",
      "settings": {
        "domainStrategy": "UseIP"
      }
    }
  ]
}
